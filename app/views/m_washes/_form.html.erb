<%= form_for(@m_wash) do |f| %>
  <% if @m_wash.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@m_wash.errors.count, "error") %> prohibited this m_wash from being saved:</h2>

      <ul>
      <% @m_wash.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <table id='rounded-corner'>
  	<tr>
  	  <th><%= f.label '洗車機コード' %></th>
  	  <td><%= f.number_field :wash_cd %></td>  	  
  	</tr>
  	<tr>
  	  <th><%= f.label '洗車機名' %></th>
  	  <td><%= f.text_field :wash_name %></td>  	  
  	</tr>
  	<tr>
  	  <th><%= f.label '洗車機略称' %></th>
  	  <td><%= f.text_field :wash_ryaku %></td>  	  
  	</tr>
  	<tr>
  	  <th><%= f.label 'グループ種類' %></th>
  	  <td><%= f.number_field :wash_group %></td>  	  
  	</tr>
  	<tr>
  	  <th><%= f.label '最大数' %></th>
  	  <td><%= f.number_field :max_num %></td>  	  
  	</tr>
  </table>
  <div class="actions">
    <%= f.submit f.object.new_record? ? '追加' : '更新' %>
  </div>
<% end %>

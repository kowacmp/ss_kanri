<%= hidden_field_tag 'input_ymd', @input_ymd %>
<%= hidden_field_tag 'shop_kbn', @shop_kbn %>
<%= hidden_field_tag 'price_kbn', @price_kbn %>
<%= submit_tag  '  印  刷  ',format: 'pdf' %>
<% end %>



<script>
	$(function () {
		$('#report_view_table').tablefix({width:900,height: 600,fixRows:3, fixCols: 3});
	});
</script>
<h1>Listing m_class_checks</h1>

<table>
  <tr>
    <th>Class code</th>
    <th>Item</th>
    <th>Deleted flg</th>
    <th>Deleted at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @m_class_checks.each do |m_class_check| %>
  <tr>
    <td><%= m_class_check.class_code %></td>
    <td><%= m_class_check.item %></td>
    <td><%= m_class_check.deleted_flg %></td>
    <td><%= m_class_check.deleted_at %></td>
    <td><%= link_to 'Show', m_class_check %></td>
    <td><%= link_to 'Edit', edit_m_class_check_path(m_class_check) %></td>
    <td><%= link_to 'Destroy', m_class_check, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />


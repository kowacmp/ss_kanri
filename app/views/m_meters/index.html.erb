<h1>Listing m_meters</h1>

<table>
  <tr>
    <th>M shop</th>
    <th>M oil</th>
    <th>M tank</th>
    <th>Pos class</th>
    <th>Meter no</th>
    <th>Deleted flg</th>
    <th>Deleted at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @m_meters.each do |m_meter| %>
  <tr>
    <td><%= m_meter.m_shop_id %></td>
    <td><%= m_meter.m_oil_id %></td>
    <td><%= m_meter.m_tank_id %></td>
    <td><%= m_meter.pos_class %></td>
    <td><%= m_meter.meter_no %></td>
    <td><%= m_meter.deleted_flg %></td>
    <td><%= m_meter.deleted_at %></td>
    <td><%= link_to 'Show', m_meter %></td>
    <td><%= link_to 'Edit', edit_m_meter_path(m_meter) %></td>
    <td><%= link_to 'Destroy', m_meter, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New M meter', new_m_meter_path %>

<table id='rounded-corner' width="430">
  <tr>
    <th width="20%">タンクNo</th>
    <th width="27%">タンク名</th>
    <th width="27%">タンク容量</th>
    <th width="13%">編集</th>
    <th width="13%">削除</th>
  </tr>
</table>

<div style="height:200px;width:455px;overflow-y:auto;overflow-x:hidden;">
<table id='rounded-corner2'style="margin-top:-1px; width:430px;">
<% @m_tanks.each do |m_tank| %>
  <tr>
    <td width="20%"><%= m_tank.tank_no %></td>
    <td width="27%"><%= m_tank.tank_name %></td>
    <td width="27%"><%= m_tank.volume %></td>
    <td  width="13%" align="center">
      <%= link_to image_tag('icon/edit.png'), {:action => 'reserve_edit', :id => m_tank.id}, {:remote => true } %>
    </td>
    <td  width="13%" align="center">
      <%= link_to image_tag('icon/delete.png'), {:action => 'reserve_delete', :id => m_tank.id}, {:remote => true, :confirm => '削除します。よろしいですか？' } %>
    </td>
  </tr>
<% end %>
</table>
</div>
<br/>
<% params[:datas].each{|key, data| %>
	<% label_id = "div.bodyDiv #baito_#{@input_day[6,2].to_i}_#{data[:user_id]}" %> 

	<% if data[:all_work_time].to_f > 0 %>
		$("<%= label_id %>").text("<%= data[:all_work_time] %>");
	<% else %>
		$("<%= label_id %>").text("");
	<% end %>
	
	<% select_where = " duty_nengetu = '#{@input_day[0,6]}' and user_id = #{data[:user_id]} "%>
	<% jikan = d_duty_baito_total(select_where) %>

	<% label_id = "div.colDiv #baito_jikan_#{data[:user_id]}" %> 

	$("<%= label_id %>").text("<%= jikan[0].jikan %>");
	
	
<% } %>

//合計を計算
$("div.colDiv #col_baito_jikan_kei").trigger("click");
$("div#modal div.background").trigger("click");
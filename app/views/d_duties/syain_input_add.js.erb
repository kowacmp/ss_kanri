<% params[:datas].each{|key, data| %>
    <% label_id = "div.bodyDiv #hiden_syain_#{@input_day[6,2].to_i}_#{data[:user_id]}" %> 
    $("<%= label_id %>").val(<%= data[:syukin].to_i %>);

	<% label_id = "div.bodyDiv #syain_#{@input_day[6,2].to_i}_#{data[:user_id]}" %> 

	if (<%= data[:syukin].to_i %> == 1){
		$("<%= label_id %>").text("出");
		$("<%= label_id %>").css("color", "");
	}else{
		if ('<%= data[:syukin].to_s %>' != ''){
			$("<%= label_id %>").text("休");
			$("<%= label_id %>").css("color", "red");
		};
	}

	<% select_where = " duty_nengetu = '#{@input_day[0,6]}' and user_id = #{data[:user_id]} "%>
	<% nisu = d_duty_syain_total(select_where) %>

	<% label_id = "div.colDiv #syain_nisu_#{data[:user_id]}" %> 
	$("<%= label_id %>").text("<%= nisu[0].nisu %>");
	
	
<% } %>

//合計を計算
$("div.colDiv #col_syain_nisu_kei").trigger("click");
$("div#modal div.background").trigger("click");
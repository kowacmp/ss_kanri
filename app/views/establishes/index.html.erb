<h1>Listing establishes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Zip cd</th>
    <th>Address</th>
    <th>System name</th>
    <th>Tax rate</th>
    <th>Limit</th>
    <th>Deleted flg</th>
    <th>Deleted at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @establishes.each do |establish| %>
  <tr>
    <td><%= establish.name %></td>
    <td><%= establish.zip_cd %></td>
    <td><%= establish.address %></td>
    <td><%= establish.system_name %></td>
    <td><%= establish.tax_rate %></td>
    <td><%= establish.limit %></td>
    <td><%= establish.deleted_flg %></td>
    <td><%= establish.deleted_at %></td>
    <td><%= link_to 'Show', establish %></td>
    <td><%= link_to 'Edit', edit_establish_path(establish) %></td>
    <td><%= link_to 'Destroy', establish, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Establish', new_establish_path %>

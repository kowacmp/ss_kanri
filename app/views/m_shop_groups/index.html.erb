<h1>Listing m_shop_groups</h1>

<table>
  <tr>
    <th>Group cd</th>
    <th>Group name</th>
    <th>Deleted flg</th>
    <th>Deleted at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @m_shop_groups.each do |m_shop_group| %>
  <tr>
    <td><%= m_shop_group.group_cd %></td>
    <td><%= m_shop_group.group_name %></td>
    <td><%= m_shop_group.deleted_flg %></td>
    <td><%= m_shop_group.deleted_at %></td>
    <td><%= link_to 'Show', m_shop_group %></td>
    <td><%= link_to 'Edit', edit_m_shop_group_path(m_shop_group) %></td>
    <td><%= link_to 'Destroy', m_shop_group, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

